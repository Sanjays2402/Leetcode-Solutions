class Solution {
public:
    char findTheDifference(string s, string t) {
        
        map<char,int> hash;
        for(int i=0; i<s.size(); i++)
            hash[s[i]]++;
        for(int i=0; i<t.size(); i++)
            hash[t[i]]++;
        
        for(char a='a'; a<='z'; a++)
        {
            if(hash[a]%2)
                return a;
        }
        return 'a';
    }
};
