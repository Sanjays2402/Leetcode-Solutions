class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        
        unordered_map<int,int> mappy;
        
        for(int i = 0; i < nums.size(); i++)
        {
            if(mappy.find(nums[i]) != mappy.end())
            {
                return {mappy.find(nums[i])->second, i};
            }
            else
            {
                mappy[target - nums[i]] = i;
            }
        }
        return{};
    }
};
