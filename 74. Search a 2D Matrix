class Solution {
public:
    bool searchMatrix(vector<vector<int>>& matrix, int target) {
        
        if(matrix.size()==0 || matrix[0].size()==0)
            return false; 
        int m=matrix.size(), n=matrix[0].size();
        int i=m-1, j=n-1;
        if(target > matrix[i][j])
            return false;
        i=0, j=n-1;
        while(i<m && j>=0)
        {
            if(target == matrix[i][j])
                return true;
            else if(target < matrix[i][j])
                j--;
            else if(target > matrix[i][j])
                i++;
        }
        
        return false;
    }
};
