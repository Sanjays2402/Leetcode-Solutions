/*
// Definition for a Node.
class Node {
public:
    int val;
    vector<Node*> children;

    Node() {}

    Node(int _val) {
        val = _val;
    }

    Node(int _val, vector<Node*> _children) {
        val = _val;
        children = _children;
    }
};
*/

class Solution {
public:
    vector<int> result;
    void preorder1(Node* root)
    {
        if(root == NULL)
            return;
        
        result.push_back(root->val);
        for(auto i: root->children)
        {
            preorder1(i);
        }
    }
    vector<int> preorder(Node* root) {
        
        if(root == NULL)
            return {};
        
        preorder1(root);
        
        return result;
    }
};
